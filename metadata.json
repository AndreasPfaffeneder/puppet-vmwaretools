{
  "name": "razorsedge-vmwaretools",
  "version": "4.8.1",
  "author": "razorsedge",
  "summary": "Install VMware Tools Operating System Specific Packages. OSPs are an alternative to installing VMware Tools from the VI Client. VMware Tools OSPs allow you to use your Operating System's native update mechanisms to automatically download, install, and manage VMware Tools for these supported Operating Systems. This enables managing VMware Tools from within the guest in the same way as other standard software.",
  "license": "Apache-2.0",
  "project_page": "https://github.com/razorsedge/puppet-vmwaretools",
  "issues_url": "https://github.com/razorsedge/puppet-vmwaretools/issues",
  "tags": [ "virtualization", "vmware", "vmware-tools", "vmware_tools", "vmtools", "RHEL", "rhel", "OEL", "CentOS", "SuSE" ],
  "source": "https://github.com/razorsedge/puppet-vmwaretools.git",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=2.3.0 <5.0.0"
    },
    {
      "name": "wolfspyre/vmware_puppetfact",
      "version_requirement": ">=0.2.1"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6" ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [ "5", "6" ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=2.7.20 <4.0.0"
    }
  ]
}
